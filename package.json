        "@types/yargs": "^17.0.24",
        "@types/yarnpkg__lockfile": "^1.1.6",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "esbuild": "^0.18.4",
        "eslint": "^8.43.0",
        "glob": "^10.2.7",
        "jest": "^29.5.0",
        "npm-dts": "^1.3.12",
        "ts-jest": "^29.1.0",
        "typescript": "^5.1.3",
        "tsup": "7.2.0"
